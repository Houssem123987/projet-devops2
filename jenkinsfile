pipeline{
    agent any


    environment {
        Docker_Image = "houssem128/myapp:1.0"
    }

    stages {
        stage ('Clone') {
            git branch: 'main' , url:
        }
        stage('SonarQube Analysis') {
           steps {
              script {
                withSonarQubeEnv('SonarQube') {
                bat 'sonar-scanner -Dsonar.login=devops-token'
            }
        }
    }
}
    }
}
